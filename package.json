    "create-torrent": "^2.0.0",
    "debug": "^1.0.2",
    "domready": "^1.0.5",
    "drag-drop": "^1.0.2",
    "express": "^4.2.0",
    "http-proxy": "^1.1.4",
    "jade": "^1.3.1",
    "posix": "^1.0.3",
    "run-auto": "^1.0.0",
    "webtorrent-tracker": "0.x"
  },
  "devDependencies": {
    "browserify": "^5.9.1",
    "nib": "^1.0.2",
    "nodemon": "^1.2.1",
    "stylus": "^0.47.1",
    "watchify": "^1.0.1"
  },
  "homepage": "http://webtorrent.io",
  "keywords": [
    "torrent",
    "bittorrent",
    "bittorrent client",
    "streaming",
