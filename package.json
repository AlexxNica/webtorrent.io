  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "nib": "^1.0.2",
    "nodemon": "^1.2.1",
    "standard": "^4.0.1",
    "stylus": "^0.51.0",
    "uglify-js": "^2.4.17",
    "watchify": "^3.1.2"
  },
  "homepage": "http://webtorrent.io",
  "keywords": [
    "torrent",
